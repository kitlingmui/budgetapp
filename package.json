{
  "name": "budgetapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "install": "^0.12.2",
    "mongoose": "^5.4.3",
    "node-pre-gyp": "^0.12.0",
    "nodemon": "^1.18.7",
    "routes": "^2.1.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "Hoang Cao, \r Kyle Macabasco, \r Taylor Darnell,\r Kit Ling Mui",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitlingmui/budgetapp.git"
  },
  "keywords": [],
